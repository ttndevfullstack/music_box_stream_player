{
  "name": "music-box-player",
  "author": "ttndev",
  "homepage": "https://musicbox.dev",
  "license": "MIT",
  "description": "A personal music streaming server that works",
  "keywords": [
    "music",
    "audio",
    "stream"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ttndevfullstack/music_box_stream_player"
  },
  "type": "module",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "lint": "eslint ./resources/assets/js/**/*.ts --no-error-on-unmatched-pattern && eslint ./cypress/**/*.ts --no-error-on-unmatched-pattern",
    "lint:fix": "eslint ./resources/assets/js/**/*.ts --no-error-on-unmatched-pattern --fix && eslint ./cypress/**/*.ts --no-error-on-unmatched-pattern --fix",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:unit": "vitest",
    "test:e2e": "kill-port 8080 && start-test dev http-get://localhost:8000/api/ping 'cypress open'",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.php": [
      "composer cs"
    ],
    "resources/assets/**/*.ts": [
      "eslint"
    ],
    "cypress/**/*.ts": [
      "eslint"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "axios": "^0.21.1",
    "events": "^3.3.0",
    "local-storage": "^2.0.0",
    "lodash": "^4.17.21",
    "nouislider": "^15.8.1",
    "tiny-typed-emitter": "^2.1.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.6",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.4",
    "bootstrap": "^5.2.3",
    "cross-env": "^7.0.3",
    "cypress": "^13.16.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "jest-serializer-vue": "^3.1.0",
    "js-md5": "^0.8.3",
    "jsdom": "^25.0.1",
    "laravel-vite-plugin": "^1.0.0",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.38",
    "postcss-mixins": "^10.0.0",
    "postcss-nested": "^6.0.1",
    "sass": "^1.56.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^4.8.4",
    "typescript-eslint": "^8.18.2",
    "vite": "^5.0.0",
    "vitest": "^2.1.6",
    "vue": "^3.2.37",
    "yarn": "^1.22.22"
  },
  "config": {
    "preferred-install": "dist",
    "minimum-stability": "stable",
    "optimize-autoloader": true,
    "prefer-stable": false
  }
}
